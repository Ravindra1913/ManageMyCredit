<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>AllUsersServlet</servlet-name>
    <servlet-class>package2.AllUsersServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SelectedUserServlet</servlet-name>
    <servlet-class>package2.SelectedUserServlet</servlet-class>
  </servlet>
   <servlet>
    <servlet-name>CreditTransferServlet</servlet-name>
    <servlet-class>package2.CreditTransferServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>AllUsersServlet</servlet-name>
    <url-pattern>/dballusers</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>SelectedUserServlet</servlet-name>
    <url-pattern>/selecteduser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CreditTransferServlet</servlet-name>
    <url-pattern>/transfer</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>HomePage.html</welcome-file>
  </welcome-file-list>
</web-app>
